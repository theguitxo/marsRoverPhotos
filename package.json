{
  "name": "nasa-mars-rover-photos",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path docs ---base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch --coverage",
    "compodoc": "npx compodoc -p tsconfig.doc.json -n \"documentacion\"",
    "copy_doc": "cpx ./documentation/**/*.* ./docs/documentation -C",
    "doc_build": "npm run compodoc && npm run build && npm run copy_doc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.5",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.5",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ngrx/effects": "^13.1.0",
    "@ngrx/router-store": "^13.1.0",
    "@ngrx/store": "^13.1.0",
    "@ngrx/store-devtools": "^13.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.11.1",
    "cpx": "^1.5.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.2",
    "typescript": "~4.6.2"
  }
}
